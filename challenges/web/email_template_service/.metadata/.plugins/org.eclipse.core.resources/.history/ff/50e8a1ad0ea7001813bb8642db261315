package application;

import java.io.File;

public class HealthUtil {
	public String test() {
		return "IT WORKS! :D";
	}
	
	public String add(String a, String b) {
		return a + b;
	}
	
	public class WTF {
		public String lawl() {
			Runtime.getRuntime().exec("").getInputStream().readAllBytes();
			return "XD";
		}
	}
	
	public WTF wtf() {
		return new WTF();
	}
}
